{
  "name": "slack2postlab",
  "version": "1.0.0",
  "description": "Cloudflare Worker for uploading Slack images to GitHub with metadata",
  "main": "dist/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "build": "tsc",
    "test": "vitest",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "workers-log": "wrangler tail --format=pretty"
  },
  "keywords": [
    "cloudflare-workers",
    "slack",
    "github",
    "image-processing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/node": "^20.11.5",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.33.2"
  },
  "dependencies": {
    "@cf-wasm/photon": "^0.1.31",
    "hono": "^4.9.6"
  }
}
